<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sintel.webcontentanalyzer">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".FulfuldeAnalysis"
            android:label="Fulfulde Text Analyzer">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.sintel.webcontentanalyzer.MainActivity" />
        </activity>
        <activity
            android:name=".Help_Activity"
            android:label="Guide d&apos;utilisation">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.sintel.webcontentanalyzer.MainActivity" />
        </activity>
        <activity
            android:name=".APropos"
            android:label="A Propos">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.sintel.webcontentanalyzer.MainActivity" />
        </activity>
        <activity
            android:name=".ClassifierActivity"
            android:label="Resultat classification" />
        <activity
            android:name=".AnalyseActivity"
            android:label="Analyse de la page" />
        <activity
            android:name=".HistoriqueActivity"
            android:label="Historique">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.sintel.webcontentanalyzer.MainActivity" />
        </activity>
        <activity android:name=".HTMLActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.sintel.webcontentanalyzer.MainActivity" />
        </activity>
        <activity
            android:name=".LinksActivity"
            android:label="Extraction des liens">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.sintel.webcontentanalyzer.MainActivity" />
        </activity>
        <activity
            android:name=".ImagesActivity"
            android:label="Extraction des images">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.sintel.webcontentanalyzer.MainActivity" />
        </activity>
        <activity
            android:name=".ContentActivity"
            android:label="Extraction de texte">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.sintel.webcontentanalyzer.MainActivity" />
        </activity>
        <activity
            android:name=".HomeActivity"
            android:theme="@style/Theme.Design.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/MonTheme.">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
    </application>

</manifest>